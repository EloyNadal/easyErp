<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="vistaFormProductos" prefHeight="600.0" prefWidth="800.0" stylesheets="" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.easyErp.project.controller.EditarProductosController">
   <center>
		<AnchorPane>
			<children>
            <GridPane hgap="10.0" vgap="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="80.0">
              <columnConstraints>
                <ColumnConstraints halignment="LEFT" hgrow="NEVER" minWidth="52.0" />
                <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
                  <ColumnConstraints halignment="RIGHT" hgrow="NEVER" minWidth="10.0" />
                  <ColumnConstraints halignment="RIGHT" hgrow="NEVER" />
                  <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" minWidth="10.0" />
                  <ColumnConstraints halignment="RIGHT" hgrow="NEVER" minWidth="50.0" />
                  <ColumnConstraints halignment="RIGHT" hgrow="NEVER" minWidth="50.0" />
                  <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" />
                  <ColumnConstraints halignment="RIGHT" hgrow="NEVER" minWidth="10.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
				<HBox spacing="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
               </children>
				</HBox>
      				<TableView fx:id="productosTable" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="300.0">
      					<columns>
                        <TableColumn fx:id="id" editable="false" maxWidth="100.0" minWidth="50.0" prefWidth="50.0" text="id" />
                        <TableColumn fx:id="nombre" maxWidth="150.0" minWidth="100.0" prefWidth="100.0" text="Nombre" />
      						<TableColumn fx:id="categoria" maxWidth="100.0" minWidth="50.0" prefWidth="50.0" text="Categoria" />
      						<TableColumn fx:id="ean13" maxWidth="300.0" minWidth="100.0" prefWidth="100.0" text="Ean13" />
      						<TableColumn fx:id="referencia" maxWidth="600.0" minWidth="100.0" prefWidth="200.0" text="Referencia" />
      						<TableColumn fx:id="atributoValor" maxWidth="150.0" minWidth="100.0" prefWidth="100.0" styleClass="" text="Atributo" />
                        <TableColumn fx:id="precio" maxWidth="150.0" minWidth="100.0" prefWidth="100.0" styleClass="" text="Precio" />
                        <TableColumn fx:id="iva" maxWidth="150.0" minWidth="100.0" prefWidth="100.0" styleClass="" text="Iva" />
                        <TableColumn fx:id="activo" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" text="Activo" />
                        <TableColumn fx:id="ver" maxWidth="150.0" minWidth="50.0" prefWidth="100.0" styleClass="" text="ver" />
      					</columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
      				</TableView>
			</children>
         <BorderPane.margin>
            <Insets left="1.0" right="1.0" />
         </BorderPane.margin>
		</AnchorPane>
   </center>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</BorderPane>
